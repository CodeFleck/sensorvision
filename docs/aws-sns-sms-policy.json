{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSNSSMSPublish",
            "Effect": "Allow",
            "Action": [
                "sns:Publish"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "sns:Protocol": "sms"
                }
            }
        }
    ]
}
